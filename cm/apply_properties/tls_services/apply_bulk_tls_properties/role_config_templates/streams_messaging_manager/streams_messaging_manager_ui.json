{
    "refName": "{{ ref_name }}",
    "roleType": "STREAMS_MESSAGING_MANAGER_UI",
    "base": true, 
    "configs": [
        {"name": "ssl_server_privatekey_location", "value": "{{ cluster_ssl_cert_path }}{{ cluster_key_pem_filename }}"},
        {"name": "ssl_server_certificate_location", "value": "{{ cluster_ssl_cert_path }}{{ cluster_cert_filename }}"},
        {"name": "ssl_client_truststore_location", "value": "{{ cluster_keystore_path }}{{ cluster_truststore_filename }}"},
        {"name": "ssl_server_ca_certificate_location", "value": "{{ cluster_ssl_cert_path }}{{ cluster_ca_cert_filename }}"},
        {"name": "ssl_enabled", "value": "true"}, 
        {"name": "ssl_server_privatekey_password", "value": "{{ keystore_password }}"}
    ]
}